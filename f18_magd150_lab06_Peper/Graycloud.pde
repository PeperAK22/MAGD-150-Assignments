Class Graycloud;

  //void Graycloud is creating the ellipses and color for the function Graycloud();.
  void Graycloud(){
    fill(150);
    stroke(150);
    ellipse(70, 150, 60, 60);
    ellipse(100, 130, 70, 70);
    ellipse(120, 160, 70, 60);
    ellipse(130, 135, 70, 70);
    ellipse(160, 150, 60, 60);
  }
  
  
  
  
  
  
  
